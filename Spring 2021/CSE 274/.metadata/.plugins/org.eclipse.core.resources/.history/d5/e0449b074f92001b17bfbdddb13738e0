
public class BinaryNode {
	// ==================== Properties ====================
	int data;
	BinaryNode left;
	BinaryNode right;
	
	
	// ==================== Constructors ====================
	BinaryNode(int data) {
		this.data = data;
		this.left = null;
		this.right = null;
	}
	
	
	// ==================== Methods ====================
	
	
}
